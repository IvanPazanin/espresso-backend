{
  // Use IntelliSense to find out which attributes exist for C# debugging
  // Use hover for the description of the existing attributes
  // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Web Api",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build web-api",
      "program": "${workspaceFolder}/source/Espresso.WebApi/bin/Debug/net5.0/Espresso.WebApi.dll",
      "args": ["--urls", "http://localhost:8000/"],
      "cwd": "${workspaceFolder}/source/Espresso.WebApi",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "^\\s*Now listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "local-local-db"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      },
      "logging": {
        "engineLogging": false,
        "moduleLoad": false,
        "exceptions": false,
        "browserStdOut": false
      },
      "requireExactSource": false
    },
    {
      "name": "Parser",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build parser",
      "program": "${workspaceFolder}/source/Espresso.ParserDeleter/bin/Debug/net5.0/Espresso.ParserDeleter.dll",
      "args": ["--urls", "http://localhost:9000/"],
      "cwd": "${workspaceFolder}/source/Espresso.ParserDeleter",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "Started Parser"
      },
      "env": {
        "DOTNET_ENVIRONMENT": "local-local-db"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      },
      "logging": {
        "engineLogging": false,
        "moduleLoad": false,
        "exceptions": false,
        "browserStdOut": false
      }
    }
  ]
}
