{
  // Use IntelliSense to find out which attributes exist for C# debugging
  // Use hover for the description of the existing attributes
  // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Web Api",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build web-api",
      // If you have changed target frameworks, make sure to update the program path.
      "program": "${workspaceFolder}/source/Espresso.WebApi/bin/Debug/netcoreapp3.1/Espresso.WebApi.dll",
      "args": ["--urls", "http://localhost:8000/"],
      "cwd": "${workspaceFolder}/source/Espresso.WebApi",
      "stopAtEntry": false,
      // Enable launching a web browser when ASP.NET Core starts. For more information: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "^\\s*Now listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "local-local-db"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      },
      "logging": {
        "engineLogging": false,
        "moduleLoad": false,
        "exceptions": false,
        "browserStdOut": false
      }
    },
    {
      "name": "Parser",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build parser",
      // If you have changed target frameworks, make sure to update the program path.
      "program": "${workspaceFolder}/source/Espresso.ParserDeleter/bin/Debug/netcoreapp3.1/Espresso.ParserDeleter.dll",
      "args": [],
      "cwd": "${workspaceFolder}/source/Espresso.ParserDeleter",
      "stopAtEntry": false,
      // Enable launching a web browser when ASP.NET Core starts. For more information: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "Started Parser"
      },
      "env": {
        "DOTNET_ENVIRONMENT": "local-local-db"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      },
      "logging": {
        "engineLogging": false,
        "moduleLoad": false,
        "exceptions": false,
        "browserStdOut": false
      }
    }
  ]
}
