{
  "https_port": 443,
  "AppConfiguration": {
    "Environment": 1
  },
  "CronJobsConfiguration": {
    "ApplicationDownloadStatisticsCronExpression": "* * * * *",
    "WebApiReportCronExpression": "* * * * *"
  },
  "DatabaseConfiguration": {
    "DefaultConnectionString": "DefaultConnectionStringPlaceholder",
    "EnableDetailedErrors": true,
    "EnableSensitiveDataLogging": true
  },
  "SpaConfiguration": {
    "UseSpaProxyServer": false,
    "SpaProxyServerUrl": "http://docker.for.mac.localhost:3000"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore": "None",
      "System": "Warning",
      "Espresso.Application.Infrastructure.MediatorInfrastructure.LoggerPipeline": "Information",
      "Espresso.Application.Infrastructure.CronJobsInfrastructure.CronJob": "Information"
    }
  }
}
