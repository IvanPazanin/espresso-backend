  },
  "SpaConfiguration": {
    "SpaProxyServerUrl": "http://docker.for.mac.localhost:3000",
    "UseSpaProxyServer": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Espresso.Application.Infrastructure.CronJobsInfrastructure.CronJob": "Information",
      "Espresso.Application.Infrastructure.MediatorInfrastructure.ExceptionRequestPipeline": "Information",
      "Espresso.Application.Infrastructure.MediatorInfrastructure.LoggerRequestPipeline": "Information",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "System": "Warning"
    }
  }
