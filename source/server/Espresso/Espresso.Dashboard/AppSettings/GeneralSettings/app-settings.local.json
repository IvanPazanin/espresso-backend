{
  "AppConfiguration": {
    "ServerUrl": "http://espresso-webapi:80",
    "Environment": 1,
    "MaxAgeOfArticlesInDays": 2
  },
  "ArticleSimilarityConfiguration": {
    "ArticlePublishDateTimeDiferenceThresholdInHours": 6,
    "MaxAgeOfSimilarArticleCheckingInHours": 26
  },
  "CronJobsConfiguration": {
    "DeleteArticlesCronExpression": "* * * * *",
    "ParseArticlesCronExpression": "* * * * *"
  },
  "HttpClientConfiguration": {
    "LoadRssFeedsHttpClientConfiguration": {
      "MaxRetries": 1,
      "TimeoutInSeconds": 60
    },
    "ScrapeWebHttpClientConfiguration": {
      "MaxRetries": 1,
      "TimeoutInSeconds": 60
    }
  },
  "JwtConfiguration": {
    "Issuers": ["localhost:9000"],
    "Audiences": ["localhost:9000"]
  }
}
