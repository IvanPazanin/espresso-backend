{
  "AppConfiguration": {
    "ServerUrl": "http://webapi:80",
    "Environment": 2,
    "MaxAgeOfArticlesInDays": 3
  },
  "DatabaseConfiguration": {
    "DefaultConnectionString": "DefaultConnectionStringPlaceholder",
    "EnableDetailedErrors": true,
    "EnableSensitiveDataLogging": true
  },
  "CronJobsConfiguration": {
    "ParserDeleterReportCronExpression": "0 8 1 * *",
    "DeleteArticlesCronExpression": "0 * * * *",
    "ParseArticlesCronExpression": "*/10 * * * *",
    "DeleteArticlesCancellationInSeconds": 360,
    "ParseArticlesCancellationInSeconds": 360
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning",
      "Espresso.Application.Infrastructure.MediatorInfrastructure.LoggerPipeline": "Information",
      "Espresso.ParserDeleter.Application.CQRS.RssFeeds.Commands.ParseRssFeeds.ParseRssFeedsCommandHandler": "None",
      "Espresso.ParserDeleter.Application.Services.ScrapeWebService": "None",
      "Espresso.ParserDeleter.Application.Services.LoadRssFeedsService": "None",
      "Espresso.ParserDeleter.Application.Services.CreateArticleService": "None"
    }
  }
}
